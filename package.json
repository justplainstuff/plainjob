    "@types/node": "^20",
    "@vitest/coverage-v8": "^2.0.3",
    "tsup": "^8.2.4",
    "tsx": "^4.16.2",
    "vitest": "^2.0.1"
  },
  "dependencies": {
    "cron-parser": "^4.9.0"
  }
}
